<UserControl x:Class="PubSub.Client.View.ShowPublishedMessageUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PubSub.Client.View"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             xmlns:viewModels="clr-namespace:PubSub.Client.ViewModel">
    <UserControl.DataContext>
        <viewModels:ShowPublishedMessageViewModel/>
    </UserControl.DataContext>
    <Grid>
        <TextBox Text="{Binding PublishedMessage}" Height="100"></TextBox>
    </Grid>
</UserControl>
